#include <stdio.h>
/**
 *multiply_numbers - Multiply two integers together
 *
 *main - the entry of my code
 *
 *@a: The first integer
 *@b: The second integer
 *
 *Return: 0
 */
int multiply_numbers(int a, int b)
{
return (a * b);
}
int main(void)
{
int output;
int a = 9;
int b = 7;

output = multiply_numbers(a, b);
printf("The product of %d and %d is %d\n", a, b, output);

return (0);
}
